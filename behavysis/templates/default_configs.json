{
  "user": {
    "format_vid": {
      "height_px": 540,
      "width_px": 960,
      "fps": null,
      "start_sec": null,
      "stop_sec": null
    },
    "run_dlc": {
      "model_fp": "Z:\\PRJ-BowenLab\\DeepLabCut-Projects\\AGGRESSION_960px\\config.yaml"
    },
    "calculate_params": {
      "from_likelihood": {
        "window_sec": 1,
        "pcutoff": 0.8,
        "bodyparts": "--bodyparts_simba"
      },
      "stop_frame_from_dur": {
        "dur_sec": 6000
      },
      "px_per_mm": {
        "pt_a": "TopLeft",
        "pt_b": "TopRight",
        "dist_mm": 400
      }
    },
    "preprocess": {
      "interpolate": {
        "pcutoff": 0.5
      },
      "refine_ids": {
        "marked": "mouse1marked",
        "unmarked": "mouse2unmarked",
        "marking": "AnimalColourMark",
        "window_sec": 0.5,
        "metric": "rolling",
        "bodyparts": "--bodyparts_centre"
      }
    },
    "evaluate_vid": {
      "funcs": ["keypoints", "analysis"],
      "pcutoff": 0.5,
      "colour_level": "individuals",
      "radius": 4,
      "cmap": "rainbow",
      "padding": 30
    },
    "extract_features": {
      "individuals": ["mouse1marked", "mouse2unmarked"],
      "bodyparts": "--bodyparts_simba"
    },
    "classify_behavs": [
      {
        "proj_dir": "Z:\\PRJ-BowenLab\\TimLee\\resources\\behav_models\\behav_aggression\\behav_models",
        "behav_name": "aggression",
        "pcutoff": -1,
        "min_window_frames": "--min_window_frames",
        "user_defined": "--user_defined"
      },
      {
        "proj_dir": "Z:\\PRJ-BowenLab\\TimLee\\resources\\behav_models\\behav_fight\\behav_models",
        "behav_name": "fight",
        "pcutoff": -1,
        "min_window_frames": "--min_window_frames",
        "user_defined": "--user_defined"
      },
      {
        "proj_dir": "/run/user/1000/gvfs/smb-share:server=shared.sydney.edu.au,share=research-data/PRJ-BowenLab/TimLee/resources/behav_models/behav_fight/behav_models/",
        "behav_name": "fight",
        "pcutoff": -1,
        "min_window_frames": "--min_window_frames",
        "user_defined": "--user_defined"
      }
    ],
    "analyse": {
      "in_roi": [
        {
          "roi_name": "thigmo",
          "is_in": false,
          "thresh_mm": -5,
          "roi_corners": "--corners",
          "bodyparts": "--bodyparts_front"
        },
        {
          "roi_name": "centre_crossing",
          "is_in": true,
          "thresh_mm": -20,
          "roi_corners": "--corners",
          "bodyparts": "--bodyparts_front"
        }
      ],
      "speed": {
        "smoothing_sec": 1,
        "bodyparts": "--bodyparts_centre"
      },
      "social_distance": {
        "smoothing_sec": 1,
        "bodyparts": "--bodyparts_centre"
      },
      "freezing": {
        "window_sec": 2,
        "thresh_mm": 5,
        "smoothing_sec": 0.2,
        "bodyparts": "--bodyparts_simba"
      },
      "bins_sec": [30, 60, 120, 300],
      "custom_bins_sec": [60, 120, 300, 600]
    }
  },
  "ref": {
    "bodyparts_centre": [
      "LeftFlankMid",
      "BodyCentre",
      "RightFlankMid",
      "LeftFlankRear",
      "RightFlankRear",
      "TailBase1"
    ],
    "bodyparts_simba": [
      "LeftEar",
      "RightEar",
      "Nose",
      "BodyCentre",
      "LeftFlankMid",
      "RightFlankMid",
      "TailBase1",
      "TailTip4"
    ],
    "bodyparts_front": ["LeftEar", "RightEar", "Nose", "BodyCentre"],
    "corners": ["TopLeft", "TopRight", "BottomRight", "BottomLeft"],
    "min_window_frames": 2,
    "user_defined": []
  }
}
